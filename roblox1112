

local Library =
            loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("Legend HandlesYT | Project New World", "BloodTheme")

        -- MAIN
        local Main = Window:NewTab("Main")
        local MainSection = Main:NewSection("Free-Gamepasses")

         local Cred = Window:NewTab("Credits")
        local CredSection = Cred:NewSection("Credits")


MainSection:NewToggle("VIP Gamepass", "Get VIP Gamepass For Free", function(state)
 if state then
  game:GetService("Players").LocalPlayer.GamepassesOwn.VIP.Value = true
else
  game:GetService("Players").LocalPlayer.GamepassesOwn.VIP.Value = false
  end
end)


